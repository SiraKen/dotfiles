{
  "Initialize": {
    "prefix": "mdinit",
    "body": ["# ${1:Title}\n", "## Subtitle"],
    "description": ""
  },
  "Create a Checkbox": {
    "prefix": "cb",
    "body": ["- [ ] ${1:text}"],
    "description": ""
  },
  "Add a link": {
    "prefix": "ln",
    "body": ["[${1:title}](${2:url})"],
    "description": ""
  },
  "Insert an image": {
    "prefix": "img",
    "body": ["![${1:title}](${2:url})"],
    "description": ""
  },
  "Create a table": {
    "prefix": "tbl",
    "body": [
      "| ${1:title} | ${2:content} |",
      "| :-- | :-- |",
      "| data | data |"
    ],
    "description": ""
  },
  "Insert a code": {
    "prefix": "code",
    "body": ["```${1:languageName}\n```"],
    "description": ""
  }
}
